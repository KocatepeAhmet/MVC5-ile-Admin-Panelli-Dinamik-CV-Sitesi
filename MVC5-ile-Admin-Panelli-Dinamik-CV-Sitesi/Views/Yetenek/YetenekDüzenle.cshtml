@model WebApplication1.Models.Entity.yetenekler

@{
    ViewBag.Title = "YetenekDüzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Yetenek Düzenleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Yetenek Adı")
    @Html.TextBoxFor(x => x.yetenek, new { @class = "form-control" })
    <br />
    @Html.Label("Yetenek Derecesi")
    @Html.TextBoxFor(x => x.oran, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>